        <name>pvp</name>
        <alias>QT_TRANSLATE_NOOP("MetaData","Precio de venta")</alias>
        <null>true</null>
        <pk>false</pk>
        <type>double</type>
        <partI>6</partI>
        <partD>2</partD>
        <default>0</default>
        <visiblegrid>false</visiblegrid>
    </field>
