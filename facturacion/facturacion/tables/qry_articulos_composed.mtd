<!-- fields from table articulos -->    <field>
        <name>referencia</name>
<!-- Código de referencia del artículo	-->        <alias>QT_TRANSLATE_NOOP("MetaData","Referencia")</alias>
        <null>false</null>
        <pk>true</pk>
        <type>string</type>
        <length>18</length>
    </field>
