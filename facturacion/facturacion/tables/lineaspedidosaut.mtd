<!DOCTYPE TMD>
<TMD>
    <name>lineaspedidosaut</name>
<!-- temp. table keeping positions for automatic orders-->    <alias>QT_TRANSLATE_NOOP("MetaData","Positions of automatic orders")</alias>
    <field>
        <name>idlinea</name>
<!--Identificador interno de la tabla-->        <alias>QT_TRANSLATE_NOOP("MetaData","Identificador")</alias>
        <null>false</null>
        <pk>true</pk>
        <type>serial</type>
        <visiblegrid>false</visiblegrid>
    </field>
    <field>
        <name>idpedidoaut</name>
<!--Id del pedido relacionado -->        <alias>QT_TRANSLATE_NOOP("MetaData","Ped. Aut.")</alias>
        <null>false</null>
        <pk>false</pk>
        <type>uint</type>
        <visiblegrid>false</visiblegrid>
        <relation>
            <table>pedidosaut</table>
            <field>idpedidoaut</field>
            <card>M1</card>
            <delC>true</delC>
        </relation>
    </field>
    <field>
        <name>referencia</name>
<!--Referencia del artículo-->        <alias>QT_TRANSLATE_NOOP("MetaData","Referencia")</alias>
        <null>true</null>
        <pk>false</pk>
        <type>string</type>
        <length>18</length>
        <visible>true</visible>
        <relation>
            <table>articulos</table>
            <field>referencia</field>
            <card>M1</card>
        </relation>
    </field>
    <field>
        <name>descripcion</name>
<!--Descripción del artículo-->        <alias>QT_TRANSLATE_NOOP("MetaData","Descripción")</alias>
        <null>true</null>
        <pk>false</pk>
        <type>string</type>
        <length>100</length>
    </field>
    <field>
        <name>pvpunitario</name>
        <alias>QT_TRANSLATE_NOOP("MetaData","Precio Unidad")</alias>
        <null>false</null>
        <pk>false</pk>
        <type>double</type>
        <partI>6</partI>
        <partD>2</partD>
    </field>
    <field>
        <name>cantidad</name>
        <alias>QT_TRANSLATE_NOOP("MetaData","Cantidad")</alias>
        <null>false</null>
        <pk>false</pk>
        <type>double</type>
        <partI>6</partI>
        <partD>0</partD>
        <default>1</default>
    </field>
    <field>
        <name>pvpsindto</name>
<!--Precio unitario por cantidad-->        <alias>QT_TRANSLATE_NOOP("MetaData","Precio Nominal")</alias>
        <null>false</null>
        <pk>false</pk>
        <type>double</type>
        <partI>6</partI>
        <partD>2</partD>
    </field>
    <field>
        <name>pvptotal</name>
<!--Precio unitario por cantidad-->        <alias>QT_TRANSLATE_NOOP("MetaData","Total")</alias>
        <null>false</null>
        <pk>false</pk>
        <type>double</type>
        <partI>6</partI>
        <partD>2</partD>
        <calculated>true</calculated>
        <editable>false</editable>
    </field>
    <field>
        <name>codimpuesto</name>
<!--Tipo de IVA aplicado-->        <alias>QT_TRANSLATE_NOOP("MetaData","I.V.A.")</alias>
        <null>true</null>
        <pk>false</pk>
        <type>string</type>
        <length>10</length>
        <relation>
            <table>impuestos</table>
            <field>codimpuesto</field>
            <card>M1</card>
        </relation>
    </field>
    <field>
        <name>iva</name>
        <alias>QT_TRANSLATE_NOOP("MetaData","%I.V.A.")</alias>
        <null>false</null>
        <pk>false</pk>
        <type>double</type>
        <partI>2</partI>
        <partD>2</partD>
        <default>0</default>
    </field>
    <field>
        <name>dtolineal</name>
        <alias>QT_TRANSLATE_NOOP("MetaData","Dto. Lineal")</alias>
        <null>false</null>
        <pk>false</pk>
        <type>double</type>
        <partI>6</partI>
        <partD>2</partD>
        <default>0</default>
    </field>
    <field>
        <name>dtopor</name>
        <alias>QT_TRANSLATE_NOOP("MetaData","%Descuento")</alias>
        <null>false</null>
        <pk>false</pk>
        <type>double</type>
        <partI>2</partI>
        <partD>2</partD>
        <default>0</default>
    </field>
    <field>
        <name>recargo</name>
        <alias>QT_TRANSLATE_NOOP("MetaData","%R.Equivalencia")</alias>
        <null>true</null>
        <pk>false</pk>
        <type>double</type>
        <visiblegrid>false</visiblegrid>
        <partI>2</partI>
        <partD>2</partD>
    </field>
    <field>
        <name>refproveedor</name>
<!-- Código de referencia del artículo para el proveedor -->        <alias>QT_TRANSLATE_NOOP("MetaData","Ref. proveedor")</alias>
        <null>true</null>
        <pk>false</pk>
        <type>string</type>
        <length>18</length>
    </field>
</TMD>
