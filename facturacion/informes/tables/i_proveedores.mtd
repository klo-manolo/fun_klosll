<!DOCTYPE TMD>
<TMD>
	<name>i_proveedores</name>
	<!-- Listado de proveedores-->
	<alias>QT_TRANSLATE_NOOP("MetaData","Informes de proveedores")</alias>
	
	<field>
		<name>id</name>
		<!-- Identificador interno	-->
		<alias>QT_TRANSLATE_NOOP("MetaData","ID")</alias>
		<null>false</null>
		<pk>true</pk>
		<type>serial</type>
		<visiblegrid>false</visiblegrid>
	</field>
	
	<field>
		<name>descripcion</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Descripción")</alias>
		<pk>false</pk>
		<null>false</null>
		<type>string</type>
		<length>200</length>
	</field>
	
	<field>
		<name>d_proveedores_codproveedor</name>
		<!-- Proveedor inicial-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Desde proveedor")</alias>
		<pk>false</pk>
		<type>string</type>
		<length>6</length>
		
		<relation>
			<table>proveedores</table>
			<field>codproveedor</field>
			<card>M1</card>
		</relation>
	</field>
	
	<field>
		<name>h_proveedores_codproveedor</name>
		<!-- Proveedor inicial-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Hasta proveedor")</alias>
		<pk>false</pk>
		<type>string</type>
		<length>6</length>
		
		<relation>
			<table>proveedores</table>
			<field>codproveedor</field>
			<card>M1</card>
		</relation>
	</field>
	
