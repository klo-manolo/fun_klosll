		<!-- Serie-->
		<alias>QT_TRANSLATE_NOOP("MetaData","País")</alias>
		<pk>false</pk>
		<type>string</type>
		<length>10</length>
		
		<relation>
			<table>paises</table>
			<field>codpais</field>
			<card>M1</card>
		</relation>
	</field>
	
